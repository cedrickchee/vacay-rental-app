{
  "name": "@vacay/controller",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "introspect": "apollo-codegen introspect-schema http://localhost:4000 --output schema.json",
    "generate": "apollo-codegen generate src/**/*.tsx --schema schema.json --target ts-modern",
    "gen-types": "npm run introspect && npm run generate"
  },
  "dependencies": {
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "react": "^16.4.2",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "apollo-codegen": "0.19.0",
    "rimraf": "^2.6.2",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.12.0",
    "typescript": "2.9.2"
  }
}
